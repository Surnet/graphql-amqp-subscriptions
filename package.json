{
  "name": "graphql-amqp-subscriptions",
  "description": "GraphQL AMQP Subscriptions",
  "version": "1.0.7",
  "author": "https://github.com/Surnet/graphql-amqp-subscriptions/graphs/contributors",
  "license": "MIT",
  "homepage": "https://github.com/Surnet/graphql-amqp-subscriptions",
  "bugs": {
    "url": "https://github.com/Surnet/graphql-amqp-subscriptions/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Surnet/graphql-amqp-subscriptions.git"
  },
  "engines": {
    "node": ">= 10"
  },
  "keywords": [
    "graphql",
    "subscription",
    "api",
    "push",
    "pull",
    "amqp",
    "rabbitmq",
    "rabbit"
  ],
  "main": "lib/index.js",
  "scripts": {
    "start": "nodemon --ext js,ts,tsx,json --exec ts-node -- src/index.ts --extensions \".ts,.tsx\" --source-maps inline",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "tsc",
    "vulnerabilities": "npm audit",
    "check-updates": "npm outdated",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "TS_NODE_FILES=true TS_NODE_PROJECT=\"./base.tsconfig.json\" nyc mocha",
    "release": "release-it"
  },
  "dependencies": {
    "@types/debug": "4.1.5",
    "debug": "4.1.1",
    "graphql-subscriptions": "1.1.0"
  },
  "devDependencies": {
    "@types/amqplib": "0.5.13",
    "@types/chai": "4.2.11",
    "@types/chai-as-promised": "7.1.2",
    "@types/mocha": "7.0.2",
    "@types/sinon": "9.0.0",
    "@types/sinon-chai": "3.2.4",
    "amqplib": "0.5.5",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "eslint": "7.0.0",
    "graphql": "14.5.8",
    "mocha": "7.1.2",
    "mocha-tslint": "0.3.0",
    "nodemon": "2.0.3",
    "nyc": "15.0.1",
    "release-it": "13.5.8",
    "sinon": "9.0.2",
    "sinon-chai": "3.5.0",
    "ts-node": "8.10.1",
    "tslint": "6.1.2",
    "typescript": "3.8.3"
  },
  "peerDependencies": {
    "amqplib": "^0.5.3",
    "graphql": "^14.5.0"
  },
  "mocha": {
    "recursive": true,
    "spec": [
      "src/**/*.test.ts",
      "test/**/*.test.ts"
    ]
  }
}
