{
  "name": "graphql-amqp-subscriptions",
  "description": "GraphQL AMQP Subscriptions",
  "version": "1.2.1",
  "author": "https://github.com/Surnet/graphql-amqp-subscriptions/graphs/contributors",
  "license": "MIT",
  "homepage": "https://github.com/Surnet/graphql-amqp-subscriptions",
  "bugs": {
    "url": "https://github.com/Surnet/graphql-amqp-subscriptions/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Surnet/graphql-amqp-subscriptions.git"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "graphql",
    "subscription",
    "api",
    "push",
    "pull",
    "amqp",
    "rabbitmq",
    "rabbit"
  ],
  "main": "lib/index.js",
  "scripts": {
    "start": "nodemon --ext js,ts,tsx,json --exec ts-node -- src/index.ts --extensions \".ts,.tsx\" --source-maps inline",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "tsc",
    "vulnerabilities": "npm audit",
    "check-updates": "npm outdated",
    "lint": "eslint . --ext .ts",
    "test": "TS_NODE_FILES=true TS_NODE_PROJECT=\"./base.tsconfig.json\" nyc mocha",
    "release": "release-it"
  },
  "dependencies": {
    "debug": "4.3.4",
    "iterall": "1.3.0",
    "uuid": "9.0.1"
  },
  "peerDependencies": {
    "amqplib": "0.x",
    "graphql": "16.x",
    "graphql-subscriptions": "2.x"
  },
  "devDependencies": {
    "@types/amqplib": "0.10.3",
    "@types/chai": "4.2.18",
    "@types/chai-as-promised": "7.1.4",
    "@types/debug": "4.1.5",
    "@types/mocha": "8.2.2",
    "@types/node": "18.19.4",
    "@types/sinon": "10.0.0",
    "@types/sinon-chai": "3.2.8",
    "@types/uuid": "9.0.7",
    "@typescript-eslint/eslint-plugin": "6.17.0",
    "@typescript-eslint/parser": "6.17.0",
    "amqplib": "0.10.3",
    "chai": "4.3.4",
    "chai-as-promised": "7.1.1",
    "eslint": "8.56.0",
    "graphql": "16.8.1",
    "graphql-subscriptions": "2.0.0",
    "mocha": "10.2.0",
    "nodemon": "3.0.2",
    "nyc": "15.1.0",
    "release-it": "17.0.1",
    "sinon": "14.0.0",
    "sinon-chai": "3.7.0",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  },
  "mocha": {
    "recursive": true,
    "spec": ["test/**/*.test.ts"]
  }
}
