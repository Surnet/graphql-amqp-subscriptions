{
  "name": "graphql-amqp-subscriptions",
  "description": "GraphQL AMQP Subscriptions",
  "version": "2.0.0",
  "author": "https://github.com/Surnet/graphql-amqp-subscriptions/graphs/contributors",
  "license": "MIT",
  "homepage": "https://github.com/Surnet/graphql-amqp-subscriptions",
  "bugs": {
    "url": "https://github.com/Surnet/graphql-amqp-subscriptions/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Surnet/graphql-amqp-subscriptions.git"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "graphql",
    "subscription",
    "api",
    "push",
    "pull",
    "amqp",
    "rabbitmq",
    "rabbit"
  ],
  "scripts": {
    "start": "nodemon --ext js,ts,tsx,json --exec ts-node -- src/index.ts --extensions \".ts,.tsx\" --source-maps inline",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "rimraf lib && tsc --project tsconfig.build.json",
    "vulnerabilities": "npm audit",
    "check-updates": "npm outdated",
    "lint": "eslint .",
    "test": "jest --coverage",
    "prepublishOnly": "echo \"use 'npm run release'\" && exit 1",
    "release": "release-it"
  },
  "dependencies": {
    "debug": "4.3.4",
    "iterall": "1.3.0",
    "uuid": "9.0.1"
  },
  "peerDependencies": {
    "amqplib": "0.x",
    "graphql": "16.x",
    "graphql-subscriptions": "2.x"
  },
  "devDependencies": {
    "@swc/core": "1.3.104",
    "@swc/jest": "0.2.30",
    "@types/amqplib": "0.10.4",
    "@types/debug": "4.1.12",
    "@types/jest": "29.5.11",
    "@types/node": "20.11.5",
    "@types/uuid": "9.0.7",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "amqplib": "0.10.3",
    "eslint": "8.56.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-jest": "27.6.3",
    "eslint-plugin-security": "2.1.0",
    "eslint-plugin-sonarjs": "0.23.0",
    "eslint-plugin-unicorn": "50.0.1",
    "graphql": "16.8.1",
    "graphql-subscriptions": "2.0.0",
    "jest": "29.7.0",
    "nodemon": "3.0.3",
    "rimraf": "5.0.5",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  }
}
